<!-- EJS Example-->

<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
	
	<!-- These includes directly insert the HTML for the commonly used HTML code from the partials folder -->
	<% include /partials/commonHeaderContent %> 
	<% include /partials/navBar%> 
  </head>
  <body>
  
	<!-- This takes the variable from the thing calling the template in this case "title" -->
    <h1><%= title %></h1>
    <p><%= title %>, world</p>
	
	<% include /partials/bootstrapScripts %> 
  </body>
</html>
